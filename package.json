{
    "name": "openutau-yaml",
    "displayName": "OpenUTAU Highlighter",
    "description": "Language Highlighter for OpenUTAU YAML",
    "version": "0.0.2",
    "publisher": "LovelyA72",
    "repository":{ "type": "git", "url": "https://github.com/LovelyA72/vscode-ustx" },
    "license": "MIT License",
    "engines": {
        "vscode": "^1.60.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ustx",
            "aliases": ["USTX", "ustx"],
            "extensions": [".ustx"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ustx",
            "scopeName": "source.ustx",
            "path": "./syntaxes/ustx.tmLanguage.json"
        }]
    }
}